URxvt.background: [85]#000505
URxvt.foreground: #F8F8F2
URxvt.colorBD: #55FFFF
URxvt.colorIT: #34E2E2
URxvt.shading: 20
URxvt.depth: 32
URxvt.reverseVideo: false
URxvt.cursorColor: #FF55FF
URxvt.highlightColor: #ffffff
URxvt.highlightTextColor: #555555

URxvt.scrollBar: false
URxvt.letterSpace: 0
URxvt.locale: true
URxvt.font: xft:JetBrains\ Mono:size=16:antialias=true:autohint=true
!URxvt.font: xft:DejaVu\ Sans\ Mono:size=15, xft:Noto\ Color\ Emoji, xft:Font\ Awesome, xft:Noto\ Serif\ Khmer:size=13, xft:Kh\ virak\ mono\ modern:size=20:pixelsize=18!xft:Noto\ Sans\ Khmer\ UI:size=13

!!extension
URxvt.perl-ext-common : resize-font
URxvt.resize-font.step: 1
URxvt.keysym.C-minus  : resize-font:smaller
URxvt.keysym.C-equal  : resize-font:bigger
URxvt.keysym.C-0      : resize-font:reset
